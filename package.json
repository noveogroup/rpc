    "react-native-uuid": "^1.4.9",
    "uuid": "^7.0.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/kind-of": "^6.0.0",
    "@types/uuid": "^7.0.0",
    "express": "^4.17.1",
    "jest": "^29.3.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
